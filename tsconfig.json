{
	"compilerOptions": {
		"baseUrl": ".",
		"strictNullChecks": true,
		"module": "esnext",
		"target": "es2020",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"paths": {
			"@components/*": ["src/components/*"],
			"@assets/*": ["src/assets/*"],
			"@layouts/*": ["src/layouts/*"],
			"@api/*": ["src/pages/api/*"]
		}
	}
}
